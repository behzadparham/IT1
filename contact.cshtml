@{
    Page.Title = "Title goes here";
    Layout = "~/Site/_LayoutHome.cshtml";
    var result = string.Empty;

    var name = Request.Form["contactName"];
    var email = Request.Form["contactEmail"];
    var message = Request.Form["contactMessage"];
    var resourceItem = Request.Form["resourceItem"];
    if (IsPost)
    {
        result = "با تشکر از شما پیغام شما برای مدیریت سایت ارسال گردید";
        ContactHandler.Create(name, email, message);
